{
    "name": "aurion72/chartjs",
    "type": "library",
    "description": "Permet de crÃ©er des graphiques facilement en utilisant la library ChartJs.",
    "keywords": [
        "aurion72",
        "chartjs"
    ],
    "homepage": "https://github.com/Aurion72/ChartJs",
    "license": "MIT",
    "authors": [
        {
            "name": "Aurion72",
            "email": "contact@agence-aurion.fr",
            "homepage": "https://www.agence-aurion.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0",
        "php" : ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Aurion72\\ChartJs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aurion72\\ChartJs\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2@dev"
        },
        "laravel" : {
            "providers" : [
                "Aurion72\\ChartJs\\ChartJsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
